@model IEnumerable<Person>

    @{
        ViewData["Title"] = "Registered Users";
    }

    <h2>Registered Users</h2>

    <table class="table">
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
                <th>Username</th>
                <th>Password</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Firstname</td>
                    <td>@user.Lastname</td>
                    <td>@user.Emial</td>
                    <td>@user.Username</td>
                    <td>@user.Password</td>
                </tr>
            }
        </tbody>
    </table>
