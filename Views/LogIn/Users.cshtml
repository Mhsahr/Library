@model IEnumerable<Person>

@{
    ViewData["Title"] = "Registered Users";
}

<h2>Registered Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Password</td>
            </tr>
        }
    </tbody>
</table>
